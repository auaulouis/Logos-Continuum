{
  "name": "logos-web",
  "version": "1.2.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "desktop:dev": "ELECTRON_START_URL=http://localhost:3001 electron .",
    "desktop:icon": "bash ./scripts/generate-mac-icon.sh",
    "desktop:build": "yarn desktop:icon && yarn build && electron-builder --mac",
    "lint": "next lint",
    "postinstall": "patch-package"
  },
  "build": {
    "appId": "com.logos.desktop",
    "productName": "Logos",
    "directories": {
      "output": "dist-desktop"
    },
    "asarUnpack": [
      "backend/**"
    ],
    "files": [
      ".next/**/*",
      "public/**/*",
      "electron/**/*",
      "next.config.js",
      "package.json",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "../verbatim-parser ",
        "to": "backend",
        "filter": [
          "**/*",
          "!.git/**",
          "!__pycache__/**",
          "!venv/**",
          "!tmp/**",
          "!test-docs/**"
        ]
      }
    ],
    "mac": {
      "icon": "build/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    }
  },
  "dependencies": {
    "@dotlottie/player-component": "^1.0.4",
    "@types/mixpanel-browser": "^2.36.0",
    "@types/node-sass": "^4.11.2",
    "@types/react-date-range": "^1.4.1",
    "@types/react-virtualized": "^9.21.15",
    "axios": "^0.24.0",
    "date-fns": "^2.27.0",
    "docx": "^9.6.0",
    "dropbox": "^10.31.0",
    "framer-motion": "^5.3.3",
    "lottie-react": "^2.1.0",
    "mixpanel-browser": "^2.42.0",
    "multiselect-react-dropdown": "^2.0.7",
    "next": "12.0.4",
    "next-auth": "^4.9.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-date-range": "^1.4.0",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-is": "^17.0.2",
    "react-select": "^5.2.1",
    "react-virtualized": "^9.22.3",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "electron": "^32.2.0",
    "electron-builder": "^24.13.3",
    "eslint": "7",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-next": "12.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "4.5.2"
  }
}
